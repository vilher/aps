
<%- include("partials/headerLogin"); -%>
<div class="p-4 p-md-5 mb-4 text-white rounded bg-dark">
  <div class="col-md-6 px-0">
    <h1 class="display-4 fst-italic">Tavo sukurtos viktorinos</h1>
  </div>
</div>
<div class="row mb-2">
  <%var a=0;
  Quiz.forEach(function(element){a++; %>
    <div class="col-md-6" style="padding-left: 50px;padding-right: 50px;">
      <div class="row g-0 border rounded overflow-hidden flex-md-row mb-4 shadow-sm h-md-250 position-relative">
        <div class="col p-4 d-flex flex-column position-static">
          <strong class="d-inline-block mb-2 text-primary"><%=a%></strong>
          <h3 class="mb-0"><%=element.Pavadinimas%></h3>
         
          <div class="mb-1 text-muted"><%=element.Data%></div>
          <p class="card-text mb-auto"><%=element.Aprasymas%></p>
          <form action="/deleteQuiz" method="get">
          <a href="/deleteQuiz" onchange="this.form.submit()" name="delete" class="stretched-link">Trinti</a> 
          </form>                
        </div>
        <button class="w-30 btn btn-outline-primary" type="submit">Modifikuoti</button> 
      </div>
    </div>
 

   <%})%>

</div>
  <%- include("partials/footer"); -%>